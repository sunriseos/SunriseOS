+#[cfg(not(test))]
+pub fn init() {
+    use core::intrinsics::abort;
+    if let Err(err) = stdio::init() {
+        log::error!("Error initializing stdio! {:?}", err);
+        abort();
+    }
+    fs::init();
+}
+
+pub fn unsupported<T>() -> crate::io::Result<T> {
+    Err(unsupported_err())
+}
