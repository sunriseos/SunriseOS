+        if self.result.is_some() {
+            Err(Error::new(ErrorKind::InvalidInput,
+                           "invalid argument: can't kill an exited process"))
+        } else {
+            // TODO(Sunrise): We don't have a killed command :c
+            unsupported()
+        }
